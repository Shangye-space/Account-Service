{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "mocha": true,
        "node": true
    },
    "rules": {
        "newline-after-var": "off",
        "camelcase": "off",
        "mocha/no-exclusive-tests": "error"
    },
    "plugins": ["mocha"]
}
